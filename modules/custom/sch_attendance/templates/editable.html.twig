{#
/**
 * @file editable.html.twig
 * Default theme implementation to present Editable data.
 *
 * This template is used when viewing Editable pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_editable()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('editable') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
